{
    "name": "codex/bitrix24-laravel",
    "description": "Laravel integration for the Bitrix24 REST API with CRM entity helpers.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Codex\\\\Bitrix24\\\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codex\\\\Bitrix24\\\\Tests\\\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codex\\\\Bitrix24\\\\Bitrix24ServiceProvider"
            ],
            "aliases": {
                "Bitrix24": "Codex\\\\Bitrix24\\\\Facades\\\\Bitrix24"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
